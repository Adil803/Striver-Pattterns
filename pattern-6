1 2 3 4 5
1 2 3 4
1 2 3 
1 2  
1 
class Solution {

    void printTriangle(int n) {
    
    for(int i=0;i<n;i++)
        {    int count=1;
            for(int j=i;j<n;j++)
            {
                
                System.out.print(count +" ");
                count++;
            }
            
            System.out.println("");
        }
   
    }
}


















